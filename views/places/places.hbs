<div class="align-search">
    <p id="title-search" >Where would you like to travel today?</p>
    <form class="d-flex" action="/places" method="POST">
    <input class="form-control me-2 width-search" name="name" type="search" placeholder="Search Places" aria-label="Search">
    <button class="btn color-button" type="submit">Search</button>
    </form>
</div>
{{#each places}}
<div class="search-result">
    <ul>
        <a href="/places/{{this.id}}"><li>{{this.attributes.long_name}}</li></a> 
    </ul>
</div>
{{/each}}
